<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wnd.dagger.MainActivity">

    <TextView
        android:id="@+id/tv_attackpower_desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Attack Power : "/>

    <TextView
        android:id="@+id/tv_attackpower"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tv_attackpower_desc"/>


    <TextView
        android:id="@+id/tv_attackspeed_desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Attack Speed : "
        android:layout_below="@id/tv_attackpower_desc"/>

    <TextView
        android:id="@+id/tv_attackspeed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/tv_attackspeed_desc"
        android:layout_alignTop="@id/tv_attackpower_desc"/>
</RelativeLayout>
